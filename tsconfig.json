{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["/node_modules", ".astro", ".vscode",],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "$lib/*": ["src/lib/*"],
      "$components/*": ["src/components/*"],
      "$layouts/*": ["src/layouts/*"],
    },
  },
}
